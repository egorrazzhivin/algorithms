class standardscaler:
    
    def fit(self, X: np.array):

        mu = X.mean(axis=0)
        sigma = X.std(axis=0)
        return mu, sigma
    
        assert False, 'not implemented'
    
    def transform(self, X: np.array) -> np.array:
        mu, sigma = self.fit(X)
        return (X-mu)/sigma
    
